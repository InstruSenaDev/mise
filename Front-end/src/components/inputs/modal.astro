---
interface Props {
    texto? : string;
}

const {texto} = Astro.props
---


<div class="flex w-60 h-40 border border-principalGreen rounded-xl justify-center items-center text-sm uppercase p-1 overflow-hidden hover:bg-principalGreen ease-in duration-150">
    <p class="text-center font-semibold"> {texto}  </p>
</div>
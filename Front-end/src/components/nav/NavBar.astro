---
import Logo from "../logo.astro";
---

<div
  class="w-full relative shadow-[0px_16px_44px_rgba(0,_0,_0,_0.07)] rounded-2xl bg-darkslategray flex flex-col items-start justify-start gap-[24px] leading-[normal] tracking-[normal]"
>
  <section
    class="self-stretch flex flex-row items-start justify-start py-padding-navbar px-[71px] relative gap-[12px] text-center text-[9px] text-color-white font-roboto"
  >
    <div
      class="h-6 w-6 !m-[0] absolute top-[28px] right-[-12px] rounded-80xl bg-olivedrab flex flex-row items-center justify-start p-1 box-border"
    >
      <img
        class="h-4 w-4 relative overflow-hidden shrink-0"
        loading="lazy"
        alt=""
        src="/public/iconsolidchevronleft.svg"
      />
    </div>
    <div
      class="flex-1 shadow-[20px_20px_4px_rgba(0,_0,_0,_0.25)] overflow-hidden flex flex-col items-start justify-start pt-[33.3px] pb-[17.3px] pr-[7px] pl-3 gap-[4.2px]"
    >
      <div class="flex flex-row items-start justify-start py-0 px-11">
        <img
          class="h-[9.1px] w-[11.3px] relative"
          loading="lazy"
          alt=""
